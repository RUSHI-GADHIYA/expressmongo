<body>
  <div class="myG">

    <div class="dj">


      <div class="container mt-5 align-content-center text-center font-weight-bold ">


        <form id="simpleform" method="POST" action="/submit-form">
          <div class="form-group text-center ">
            <label for="exampleInputEmail1">Email address</label>
            <input style="border-radius: 50px;" type="email" class="form-control" id="exampleInputEmail1"
              aria-describedby="emailHelp" name="email" placeholder="Enter email">
            <small id="emailHelp" class="form-text text-light my-1">We'll never share your email with anyone
              else.</small>
          </div>
          <div class="form-group  text-center mx-auto" style="width: 300px;">
            <label for="exampleInputName">Name</label>
            <input style="border-radius: 50px;" type="text" class="form-control" id="uname" name="username"
              placeholder="Name">


          </div>
          <div class="form-group text-center  mx-auto" style="width: 300px;">
            <label for="exampleInputdate">Date Of Birth</label>
            <input style="border-radius: 50px;" type="date" class="form-control" name="date" id="dob"
              placeholder="Date Of Birth">


          </div>




          <div class="form-group text-center mx-auto" style="width: 300px;">
            <label for="exampleInputPassword1">Password</label>
            <input style="border-radius: 50px;" type="password" name="password" class="form-control"
              id="exampleInputPassword1" placeholder="Password">
          </div>
          <div class="form-check text-center mx-auto">
            <input style="border-radius: 50px;" type="checkbox" class="form-check-input" id="exampleCheck1">
            <label class="form-check-label" for="exampleCheck1">Check me out</label>
          </div>
          <button style="margin-top:25px ;   border-radius: 50px;      " type="submit"
            class="btn btn-success dj">Submit</button>
        </form>


      </div>








    </div>


